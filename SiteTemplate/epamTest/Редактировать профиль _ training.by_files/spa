              editEventHtml: '=',
              deleteEventHtml: '=',
              cellIsOpen: '=',
              onTimespanClick: '=',
              cellModifier: '=',
              slideBoxDisabled: '='
            },
            controller: 'MwlCalendarMonthCtrl as vm',
            link: function (scope, element, attrs, calendarCtrl) {
              scope.vm.calendarCtrl = calendarCtrl;
            },
            bindToController: true
