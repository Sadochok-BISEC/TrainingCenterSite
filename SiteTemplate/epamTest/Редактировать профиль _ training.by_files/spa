        }, e.p = "", e(e.s = 45)
    }([function (t, e, n) {
        "use strict";
        Object.defineProperty(e, "__esModule", {value: !0});
        var r = n(17), o = n(18), i = n(19), l = n(48), a = n(49), s = n(50), u = n(51), c = n(52), f = n(11),
            h = n(29), p = n(30), d = n(28), y = n(1), v = {
                Scope: y.Scope,
                create: y.create,
                find: y.find,
                query: y.query,
                register: y.register,
                Container: r.default,
                Format: o.default,
                Leaf: i.default,
                Embed: u.default,
                Scroll: l.default,
                Block: s.default,
                Inline: a.default,
                Text: c.default,
                Attributor: {Attribute: f.default, Class: h.default, Style: p.default, Store: d.default}
            };
        e.default = v
    }, function (t, e, n) {
        "use strict";

        function r(t, e) {
            var n = i(t);
            if (null == n) throw new s("Unable to create " + t + " blot");
            var r = n;
            return new r(t instanceof Node || t.nodeType === Node.TEXT_NODE ? t : r.create(e), e)
        }

        function o(t, n) {
            return void 0 === n && (n = !1), null == t ? null : null != t[e.DATA_KEY] ? t[e.DATA_KEY].blot : n ? o(t.parentNode, n) : null
        }

        function i(t, e) {
            void 0 === e && (e = p.ANY);
            var n;
            if ("string" == typeof t) n = h[t] || u[t]; else if (t instanceof Text || t.nodeType === Node.TEXT_NODE) n = h.text; else if ("number" == typeof t) t & p.LEVEL & p.BLOCK ? n = h.block : t & p.LEVEL & p.INLINE && (n = h.inline); else if (t instanceof HTMLElement) {
                var r = (t.getAttribute("class") || "").split(/\s+/);
                for (var o in r) if (n = c[r[o]]) break;
                n = n || f[t.tagName]
            }
            return null == n ? null : e & p.LEVEL & n.scope && e & p.TYPE & n.scope ? n : null
        }

        function l() {
