(function () {
  var notificationNumber = $(".notifications-count div").text();
  var notificationDropDownMenu = $("#notificDropDownMenu");
  var unreadAllMessages = $("#unreadAll");

  if (notificationNumber < 5) {
    notificationDropDownMenu.css("height", notificationNumber * 50 + 48 + "px");
  } else {
    notificationDropDownMenu.css("max-height", "248px");
  }

  unreadAllMessages.bind("click", function () {
    notificationDropDownMenu.css("height", "40px");
  });
})();;
$(function () {
    var SEPARATED_LIST_SELECTOR = '.separated-list';
    var NO_SEP_CLASS = 'no-sep';

    separatorsLookUp();
    $(window).on('resize', $.throttle(300, separatorsLookUp));

    function separatorsLookUp() {
        $(SEPARATED_LIST_SELECTOR).each(function (index, list) {
            var listWidth = Math.ceil($(list)[0].getBoundingClientRect().width);
            var listItems = $(list).children();
            listItems.removeClass(NO_SEP_CLASS);

            var firstLineWidth = 0, i;
            for (i = 0; i < listItems.length; i++) {
                var itemWith = $(listItems[i])[0].getBoundingClientRect().width;
                firstLineWidth += itemWith;
                if (firstLineWidth > listWidth && i) {
                    firstLineWidth = itemWith;
                    $(listItems[i - 1]).addClass(NO_SEP_CLASS);
                }
            }
        });
    }

    window.separatorsLookUp = separatorsLookUp;
});
;
